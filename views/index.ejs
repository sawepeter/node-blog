<div class="author">
    <h1 class="author__heading">Hi, Its Devsawe.</h1>
    <p class="author__body">Full Stack Developer.</p>
</div>

<img src="/img/hero-image.webp" alt="Developer meditating" class="hero-image" width="981" height="528">

<section class="articles">
    <h2 class="articles__heading">Latest Posts</h2>

    |<ul class="article-ul">

        <% data.forEach(post => { %>
            <li>
                <a href="#">
                    <span><%= post.title %></span>
                    <span class="article-list__date"><%= post.createdAt %></span>
                </a>
    
            </li>
         
        <% }) %>
        
    </ul>

    <a href="#" class="pagination">&lt; View Older Posts</a>

</section>